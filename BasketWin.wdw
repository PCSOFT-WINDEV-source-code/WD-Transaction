      PROCEDURE ReadStock()
      // Reads the quantities in stock
      
      HReadSeekFirst(STOCK,Fruit,"STRAWBERRY")
      IF HFound() THEN gnStrawberryStock = STOCK.Quantity
      
      HReadSeekFirst(STOCK,Fruit,"CHERRY")
      IF HFound() THEN gnCherryStock = STOCK.Quantity
      
      HReadSeekFirst(STOCK,Fruit,"ORANGE")
      IF HFound() THEN gnOrangeStock = STOCK.Quantity
      
      HReadSeekFirst(STOCK,Fruit,"APPLE")
      IF HFound() THEN gnAppleStock = STOCK.Quantity
      
      HReadSeekFirst(STOCK,Fruit,"BANANA")
      IF HFound() THEN gnBananaStock = STOCK.Quantity
      
      HReadSeekFirst(STOCK,Fruit,"GRAPES")
      IF HFound() THEN gnGrapefruitStock = STOCK.Quantity
      
      gnStrawberryCart=0
      gnCherryBasket=0
      gnOrangeBasket=0
      gnAppleCart=0
      gnBananaCart=0
      gnGrapefruitCart=0
