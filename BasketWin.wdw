    name : BTN_VALIDATE
    identifier : 0x286fdda80879e72e
    internal_properties : 
    properties :
     x : 601
     y : 198
     width : 158
     height : 36
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        internal_properties : CAAAAAgAAAD3PfhAj13+e3Tq365W/blzvSGdoN6J5K4US3kiB8bjYfoY0ANQmI+kHwpltaxV/4XJzvToP9zvc0FAUaUEhW3Nud9GGA0xCOiIYt4jowi+e0qmBhgGkQ==
        code : |1-
         IF (gnStrawberryCart+gnCherryBasket+gnOrangeBasket+gnAppleCart+gnBananaCart+gnGrapefruitCart)=0 THEN
         	Info("Your basket is empty.","No order was saved.","","Click + to add a fruit to your basket.")
         	RETURN
         END
         
         // START OF TRANSACTION ***********************************************************************************************
         HTransactionStart(gsExeDir+"Transaction.trs")
         
         // create the order
         ORDERS.Date=Today()
         ORDERS.Time=Left(Now(),4)
         HAdd(ORDERS)
         
         // Add the order lines and modify the stock
         IF NOT bAddOrderLine("STRAWBERRY",gnStrawberryStock,gnStrawberryCart) THEN RETURN
         IF NOT bAddOrderLine("CHERRY",gnCherryStock,gnCherryBasket) THEN RETURN
         IF NOT bAddOrderLine("ORANGE",gnOrangeStock,gnOrangeBasket) THEN RETURN
         IF NOT bAddOrderLine("APPLE",gnAppleStock,gnAppleCart) THEN RETURN
         IF NOT bAddOrderLine("BANANA",gnBananaStock,gnBananaCart) THEN RETURN
         IF NOT bAddOrderLine("GRAPES",gnGrapefruitStock,gnGrapefruitCart) THEN RETURN
         
         
         // CAUTION: THE CALL TO A SCREEN DURING A TRANSACTION IS USED FOR THE PURPOSE OF THE EXAMPLE ONLY
         // 				THIS IS NOT RECOMMENDED IN AN APPLICATION: 
         //				THE TRANSACTION MUST BE AS SHORT AS POSSIBLE AND MUST ONLY CONTAIN ESSENTIAL WRITE OPERATIONS
         SWITCH Open(Win_EndTrans)
         	CASE 1
         		// VALIDATED TRANSACTION ****************************************************************************************
         		HTransactionEnd()
         		
         		gnStrawberryCart = 0
         		gnCherryBasket = 0
         		gnOrangeBasket = 0
         		gnAppleCart 	= 0
         		gnBananaCart = 0
         		gnGrapefruitCart = 0
         		
         		// Reads the quantities in stock
         		ReadStock()
         
         		// Builds the captions according to the stock and basket
         		BuildCaptions()
         		
         		Info("Order saved.")
         	CASE 2
         		// TRANSACTION CANCELED ****************************************************************************************
         		HourGlass(True) 
         		HTransactionCancel()
         		HourGlass(False)
         		Info("Your order was canceled.")
         	CASE 3
         		// TRANSACTION CANCELED IMPLICITELY **************************************************************************
         		Info("An unprotected division by 0 will be done deliberately.","The program will be stopped roughly.","","The HFSQL engine will automatically cancel the transaction.")
         		x is int
         		
         		x = 1/0				// Deliberate program error
         	OTHER CASE
         		// FAILURE SIMULATION DURING A TRANSACTION *****************************************************************
         		Open(Win_Failure)	
         		
         		// If the user does not want to restart his computer
         		// TRANSACTION CANCELED ****************************************************************************************
         		HourGlass(True) 
         		HTransactionCancel()
         		HourGlass(False)
         		Info("Your order was canceled.")	
         END
         
         
         
         CASE Error:
         	// Cancel the transaction
         	HTransactionCancel()
         	RETURN
