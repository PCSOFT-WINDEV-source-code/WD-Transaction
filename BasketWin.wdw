     type_code : 14
     code : |1-
      PROCEDURE bAddOrderLine(psFruit,pnStock,pnBasket)
      // Add an order line and modify the stock
      // psFruit is the product to add
      // pnStock is the quantity in stock
      // pnBasket is the quantity added to the basket
      
      bFlag is boolean
      bFlag = True
      
      // if this product was added to the basket
      IF pnBasket>0 THEN
      	// Check the stock
      	HReadSeekFirst(STOCK,Fruit,psFruit)
      	IF HFound() THEN
      		// if there is not enough quantity in stock to supply the requested quantity
      		IF STOCK.Quantity<pnBasket THEN
      			HTransactionCancel()
      			bFlag=False
      			Info(StringBuild("%1 %2 were left when the file was read. %3 %2 are left now: another user has added this product to his basket."+CR+"We apologize but we cannot supply the requested quantity. The order was canceled.",...
      								pnStock,psFruit,STOCK.Quantity))
      								
      			// Reads the quantities in stock
      			ReadStock()	
      
      			// Builds the captions according to the stock and basket
      			BuildCaptions()
      
      
      		ELSE
      			// Add the order line
      			ORDERLINE.ORDERID=ORDERS.ORDERID
      			ORDERLINE.STOCKID=STOCK.STOCKID
      			ORDERLINE.Fruit=psFruit
      			ORDERLINE.Quantity=pnBasket
      			IF NOT HAdd(ORDERLINE) THEN
      				HTransactionCancel()
      				bFlag=False
      				Error("Unable to create an order line.",HErrorInfo())
      			END
      
      			// Decrement the stock
      			STOCK.Quantity-=pnBasket
      			IF NOT HModify(STOCK) THEN
      				HTransactionCancel()
      				bFlag=False
      				Error("Unable to modify the stock.",HErrorInfo())
      			END
      		END
      
      	// if the product does no longer exist	
      	ELSE
      		HTransactionCancel()
      		bFlag=False
      		Info(StringBuild("The %1 product is not found",psFruit))
      	END
      END
      
      // Status report
      RESULT bFlag
     type : 458752
