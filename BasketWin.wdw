     name : AddElement
     procedure_id : 1760869692957845294
     type_code : 14
     code : |1-
      PROCEDURE AddElement(psFruit)
      // Adds a fruit to the basket
      
      SWITCH Upper(psFruit)
      	CASE "STRAWBERRY"
      		IF (gnStrawberryStock-gnStrawberryCart)>0 	THEN gnStrawberryCart++
      	CASE "CHERRY"
      		IF (gnCherryStock-gnCherryBasket)>0 	THEN gnCherryBasket++
      	CASE "ORANGE"
      		IF (gnOrangeStock-gnOrangeBasket)>0	THEN gnOrangeBasket++
      	CASE "APPLE"
      		IF (gnAppleStock-gnAppleCart)>0 	THEN gnAppleCart++
      	CASE "BANANA"
      		IF (gnBananaStock-gnBananaCart)>0 	THEN gnBananaCart++
      	CASE "GRAPES"
      		IF (gnGrapefruitStock-gnGrapefruitCart)>0 	THEN gnGrapefruitCart++
      END
      
      // Builds the captions according to the stock and basket
      BuildCaptions()
     type : 458752
   -
     name : BuildCaptions
     internal_properties : CAAAAAgAAAAA2B4Qr5YbBGIN4GgkoJu7+c7rh5l84arOt40H+0tLSTwnAkzw5TL/jAzgp9Q1t4w/Z+Sw6T2+fO9/sgzfuU5pb6Prmoas1Z46Ki6IVCS7rBIdWSxfLxRxjVjW9XjHRTy2bcChqIfLjZ1WoegjXgMGvQ/STWSfnS7fvowjHXI44918u9YgGoJfMtXx+mA1jvO18Az5
     procedure_id : 1760869692957910830
     type_code : 14
     code : |1-
      PROCEDURE BuildCaptions()
      // Builds the captions according to the stock and basket
      
      STC_STRAWBERRY = StringBuild("Strawberries left during the last reading: %1"+CR+"Strawberries left if your basket is validated: %2",gnStrawberryStock,gnStrawberryStock-gnStrawberryCart)
      STC_CHERRY = StringBuild("Remaining cherries during last reading: %1"+CR+"Remaining cherries if you validate your basket: %2",gnCherryStock,gnCherryStock-gnCherryBasket)
      STC_PINEAPPLE = StringBuild("Remaining oranges during last reading: %1"+CR+"Remaining oranges if you validate your basket: %2",gnOrangeStock,gnOrangeStock-gnOrangeBasket)
      STC_APPLE = StringBuild("Apples left during the last reading: %1"+CR+"Apples left if your basket is validated: %2",gnAppleStock,gnAppleStock-gnAppleCart)
      STC_BANANA = StringBuild("Bananas left during the last reading: %1"+CR+"Bananas left if your basket is validated: %2",gnBananaStock,gnBananaStock-gnBananaCart)
      STC_GRAPES = StringBuild("Grapes left during the last reading: %1"+CR+"Grapes left if your basket is validated: %2",gnGrapefruitStock,gnGrapefruitStock-gnGrapefruitCart)
      
      STC_BASKET = StringBuild("BASKET CONTENT"+CR+CR+"%1 cherry(ies)"+CR+"%2 strawberry(ies)"+CR+"%3 orange"+CR+"%4 apple(s)"+CR+"%5 banana(s)"+CR+"%6 grape(s)" + CR +CR + "%7 products in the basket",gnCherryBasket,gnStrawberryCart,gnOrangeBasket,gnAppleCart,gnBananaCart,gnGrapefruitCart, (gnCherryBasket+gnStrawberryCart+gnOrangeBasket+gnAppleCart+gnBananaCart+gnGrapefruitCart))
      
      // If at least one product in the basket, display the Reset button
      IF (gnCherryBasket+gnStrawberryCart+gnOrangeBasket+gnAppleCart+gnBananaCart+gnGrapefruitCart) > 0
      	BTN_RESTORE..Visible = True
      END
     type : 458752
   -
     name : Remove
     procedure_id : 1760869692957976366
     type_code : 14
     code : |1-
      PROCEDURE Remove(psFruit)
      // Removes a fruit from the basket
      
      SWITCH Upper(psFruit)
      	CASE "STRAWBERRY"
      		IF gnStrawberryCart>0 	THEN gnStrawberryCart--
      	CASE "CHERRY"
      		IF gnCherryBasket>0 	THEN gnCherryBasket--
      	CASE "ORANGE"
      		IF gnOrangeBasket>0	THEN gnOrangeBasket--
      	CASE "APPLE"
      		IF gnAppleCart>0 	THEN gnAppleCart--
      	CASE "BANANA"
      		IF gnBananaCart>0 	THEN gnBananaCart--
      	CASE "GRAPES"
      		IF gnGrapefruitCart>0 	THEN gnGrapefruitCart--
      END
      
      // Builds the captions according to the stock and basket
      BuildCaptions()
