     code : |1-
      gsExeDir is string // Directory of data
      gsTransactionLog  is string // Transaction file
      gnInstanceNum is int // Number of the application instance
      
      // Number of the program instance
      gnInstanceNum = CommandLine("SETUPNUM")
      
      LOCAL
      // Language to use
      nNation is int	
      nNation = CommandLine("LANGUAGE")
      IF nNation>0 THEN
      	Nation(nNation)
      END
      
      IF InTestMode() THEN 
      	// In test mode, return the runtime directory
      	gsExeDir = CompleteDir(fCurrentDir())
      ELSE
      	// In exe mode, return the directory of the executable
      	gsExeDir =  CompleteDir(ExeInfo(exeDirectory))
      END
      
      // transaction file
      gsTransactionLog = gsExeDir+"Transaction.trs"
      
      // Automatic resume if the transaction was abnormally interrupted
      // when previously used (Transaction log on disk)
      IF HTransactionInterrupted(gsTransactionLog) THEN
      	IF YesNo("A transaction was interrupted (the transaction was neither validated nor canceled)."+CR+"This may be caused by a power outage of by a sudden stop of the task."+CR+"The last operation was ignored."+CR+"Click Yes to automatically cancel the interrupted transaction."+CR+"Click No to cancel the transaction interrupted with WDTRANS. Start WDTRANS supplied with WINDEV.") THEN
      		HTransactionCancel(gsTransactionLog)	
      	ELSE
      		EndProgram()
      	END
      END
      
      // HF
      HCreationIfNotFound("*","",hDelayedOpening)
      
      HMode("*",hModeMulti)
      HSubstDir("?",gsExeDir)
